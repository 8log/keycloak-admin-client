{
  "name": "keycloak-admin-client",
  "version": "0.5.0-dev",
  "description": "Client for contacting the Keycloak Admin REST API",
  "main": "index.js",
  "scripts": {
    "test": "tape test/*-test.js",
    "lint": "eslint lib/*.js test/*.js index.js example.js example-with-request.js",
    "prepublish": "./node_modules/nsp/bin/nsp check",
    "coverage": "./node_modules/.bin/istanbul cover tape -- test/*.js",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | coveralls",
    "docs": "./node_modules/.bin/jsdoc --verbose -d docs -t ./node_modules/ink-docstrap/template -R README.md index.js ./lib/*.js",
    "dependencyCheck": "szero . --ci"
  },
  "keywords": [
    "Keycloak",
    "Node.js",
    "REST"
  ],
  "files": [
    "package.json",
    "README.md",
    "index.js",
    "example.js",
    "lib"
  ],
  "author": {
    "name": "Red Hat, Inc."
  },
  "maintainers": {
    "name": "Lucas Holmquist",
    "email": "lholmqui@redhat.com"
  },
  "engines": {
    "node": ">= 4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "bucharest-gold/keycloak-admin-client"
  },
  "bugs": "https://github.com/bucharest-gold/keycloak-admin-client/issues",
  "license": "Apache-2.0",
  "dependencies": {
    "keycloak-request-token": "^0.1.0",
    "request": "^2.69.0"
  },
  "devDependencies": {
    "coveralls": "^2.11.9",
    "eslint": "^3.3.1",
    "eslint-config-semistandard": "^7.0.0-beta.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.1.2",
    "eslint-plugin-standard": "^2.0.0",
    "ink-docstrap": "^1.1.4",
    "istanbul": "^0.4.3",
    "jsdoc": "^3.4.0",
    "jshint": "^2.9.1",
    "nsp": "^2.3.0",
    "tape": "^4.5.1"
  }
}
